<!--index.wxml-->
<view class="page">
    <view class="page__hd"> 
        <view class="page__title">Wechat AR Demo</view>
       
    </view>
    <view class="page__bd page__bd_spacing">
        <view class="page__desc">Color Tracker</view>

        <navigator url="/pages/camera/camera?type=color" class="marginTop10">
            <button class="weui-btn" type="primary">Access a camera</button>
        </navigator>

        <navigator url="/pages/photo/photo?type=color" class="marginTop10">
            <button class="weui-btn" type="primary">Take a photo</button>
        </navigator>

        <view class="page__desc marginTop10">Face Tracker</view>

        <navigator url="/pages/camera/camera?type=face" class="marginTop10">
            <button class="weui-btn" type="warn">Access a camera</button>
        </navigator>

        <navigator url="/pages/photo/photo?type=face" class="marginTop10">
            <button class="weui-btn" type="warn">Take a photo</button>
        </navigator>

    </view>
</view>